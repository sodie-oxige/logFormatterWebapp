<%= form_with model: @user, url: users_path do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :userid %>
    <%= form.text_field :userid %>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= form.label :password_confirmation %>
    <%= form.password_field :password_confirmation %>
    <%= form.submit %>
<% end %>