<h1><%= t '.title' %></h1>
<%= form_with model: @log, url: log_path(@log) do |form| %>
    <%= form.label :name, Log.human_attribute_name(:name) %>
    <%= form.text_field :name %>
    <%= form.label :date, Log.human_attribute_name(:date) %>
    <%= form.date_field :date %>
    <%= form.label :gm, Log.human_attribute_name(:gm) %>
    <%= form.collection_select :gm, Pl.all, :id, :name, include_blank: true %>
    <%= form.label :pc, Log.human_attribute_name(:pc) %>
    <%= form.collection_select :pcs, Pc.all, :id, :name, {}, multiple: true %>
    <%= form.submit %>
<% end %>